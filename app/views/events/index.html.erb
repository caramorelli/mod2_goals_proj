

  <div id="background_img">
    <!-- <h1><font face='Fascinate Inline' color='blue' size='10'></h1> -->
    <!-- font-family: 'Faster One', cursive; -->
  

    <div id="events_calendar">
      <%= render partial: "calendar", locals: {events: @events} %>
    </div>


    <table>
      <thead>
        <tr>
          <th>Name  </th>
          <th>Start time   </th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @events.each do |event| %>
          <tr>
            <td><%= event.name %></td>
            <td><%= event.start_time.strftime("%m/%d/%Y") %></td>
            <td><%= event.start_time.strftime("%I:%M%p") %></td>
            <!-- <td><%= link_to 'Show', event %></td>
            <td><%= link_to 'Edit', edit_event_path(event) %></td>
            <td><%= link_to 'Delete', event, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <!--  maybe instead link to each goal or Goal options? -->


  </div>
